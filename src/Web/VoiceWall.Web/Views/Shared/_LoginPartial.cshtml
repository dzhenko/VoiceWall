@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
    @Html.AntiForgeryToken()

    <ul class="nav navbar-nav navbar-right">
        <li>
            @(Html.ActionLink<SettingsController>((c) => c.General(), "Profile"))
        </li>
        <li><a href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>
    </ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li> @(Html.ActionLink<AccountController>((c) => c.Register(), "Register", null, new { id = "registerLink" }))</li>
        <li> @(Html.ActionLink<AccountController>((c) => c.Login(null), "Log in", null, htmlAttributes: new { id = "loginLink" }))</li>
    </ul>
}
